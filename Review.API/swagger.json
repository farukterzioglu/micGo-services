        ],
        "summary": "Rates the review.",
        "operationId": "rateReviewReq",
        "parameters": [
          {
            "type": "string",
            "description": "id of the review",
            "name": "reviewId",
            "in": "path",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/responses/ok"
          }
        }
      }
    }
  },
  "securityDefinitions": {
    "bearer": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header"
    }
  }
